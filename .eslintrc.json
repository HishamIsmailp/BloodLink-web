{
  "extends": "next/core-web-vitals",
  "rules": {
    "react-hooks/rules-of-hooks": "off",
    "camelcase": "error",
    "spellcheck/spell-checker": ["error", { "comments": true, "strings": true, "identifiers": true }],
    "space-before-function-paren": ["error", "always"]
  },
  "plugins": ["spellcheck"]
}
